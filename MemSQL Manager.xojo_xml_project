  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>GetUserDetails</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1133352959</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub GetUserDetails()</SourceLine>
   <SourceLine>If userlist.ListIndex &gt;= 0 Then</SourceLine>
   <SourceLine>UserRights = New Xojo.Core.Dictionary</SourceLine>
   <SourceLine>UserRights.Value("Global") = New Xojo.Core.Dictionary</SourceLine>
   <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In KnownDatabases</SourceLine>
   <SourceLine>UserRights.Value(entry.key) = New Xojo.Core.Dictionary</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>UserName.Text = userlist.Cell(userlist.ListIndex,0)</SourceLine>
   <SourceLine>HostName.Text= userlist.Cell(userlist.ListIndex,1)</SourceLine>
   <SourceLine>Password.Text = "!!&lt;&lt;NOCHANGE&gt;&gt;!!"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim  results As RecordSet</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>results = app.db.SQLSelect("SHOW GRANTS FOR '"+userlist.Cell(userlist.ListIndex,0)+"'@'"+userlist.Cell(userlist.ListIndex,1)+"';")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If app.db.Error Then</SourceLine>
   <SourceLine>MsgBox("Error while searching: "+app.db.ErrorMessage)</SourceLine>
   <SourceLine>Return </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If results= Nil Or results.RecordCount=0 Then</SourceLine>
   <SourceLine>MsgBox("No results")</SourceLine>
   <SourceLine>Return </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>While Not results.eof</SourceLine>
   <SourceLine>ProcessGrants(results.idxField(1).StringValue)</SourceLine>
   <SourceLine>results.MoveNext</SourceLine>
   <SourceLine>Wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>results.Close</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Update the GUI</SourceLine>
   <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In UserRights</SourceLine>
   <SourceLine>If CType(entry.Key, String) = "Global" Then</SourceLine>
   <SourceLine>GlobalPerms.clear</SourceLine>
   <SourceLine>GlobalPerms.setPermissions(entry.Value)</SourceLine>
   <SourceLine>GlobalPerms.ExistingRights=entry.Value</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Dim sp As SchemaPermissions =SchemaPerms.Value(entry.key)</SourceLine>
   <SourceLine>sp.clear</SourceLine>
   <SourceLine>sp.setPermissions(entry.Value)</SourceLine>
   <SourceLine>sp.ExistingRights=entry.Value</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>ProcessGrants</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>848457727</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub ProcessGrants(Grants As String)</SourceLine>
   <SourceLine>Dim Patterns(-1) As String = Array( _</SourceLine>
   <SourceLine>"^grant (.*) on `?(\S*?)`?\.`?(\S*?)`? To '?(\S*?)'?\@'?(\S*?)'?\s*(IDENTIFIED BY PASSWORD \S*\s*)?(WITH GRANT OPTION)?$" _</SourceLine>
   <SourceLine>)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For Each pattern As String In Patterns</SourceLine>
   <SourceLine>Dim rg As New RegEx</SourceLine>
   <SourceLine>Dim match As RegExMatch</SourceLine>
   <SourceLine>rg.SearchPattern = pattern</SourceLine>
   <SourceLine>match = rg.Search(Grants)</SourceLine>
   <SourceLine>If match &lt;&gt; Nil Then</SourceLine>
   <SourceLine>System.DebugLog("Grants: "+Grants)</SourceLine>
   <SourceLine>System.DebugLog("Matched: "+pattern)</SourceLine>
   <SourceLine>System.DebugLog("Patterns matched: "+Str(match.SubExpressionCount))</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>For  i = 0 To match.SubExpressionCount-1</SourceLine>
   <SourceLine>System.DebugLog("Matched ["+match.SubExpressionString(i)+"]")</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim grantOption As Boolean </SourceLine>
   <SourceLine>If (match.SubExpressionCount=7) Then</SourceLine>
   <SourceLine>grantOption=(match.SubExpressionString(6) = "WITH GRANT OPTION")</SourceLine>
   <SourceLine>Elseif (match.SubExpressionCount=8) Then</SourceLine>
   <SourceLine>grantOption=(match.SubExpressionString(7) = "WITH GRANT OPTION")</SourceLine>
   <SourceLine>Elseif (match.SubExpressionCount=5) Then</SourceLine>
   <SourceLine>grantOption=(match.SubExpressionString(6) = "WITH GRANT OPTION")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>// Save it to the schema list</SourceLine>
   <SourceLine>processPrivilege(match.SubExpressionString(1),match.SubExpressionString(2), _</SourceLine>
   <SourceLine>match.SubExpressionString(3),match.SubExpressionString(4), _</SourceLine>
   <SourceLine>match.SubExpressionString(5),grantOption)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>Grants As String</ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>ProcessPrivilege</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>404428799</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub ProcessPrivilege(Rights As String, Schema As String, Item As String, User As String, Host As String, Grant As Boolean)</SourceLine>
   <SourceLine>Dim RightsMap As Xojo.Core.Dictionary</SourceLine>
   <SourceLine>If schema="*" And item="*" Then</SourceLine>
   <SourceLine>RightsMap = UserRights.Value("Global")</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>RightsMap = UserRights.Value(schema)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim rightsList(-1) As String = Rights.Split(",")</SourceLine>
   <SourceLine>For Each  priv As String In rightsList</SourceLine>
   <SourceLine>priv =priv.Trim.Uppercase</SourceLine>
   <SourceLine>If priv = "ALL PRIVILEGES" Then</SourceLine>
   <SourceLine>RightsMap.Value("RightsGroup.*") = True</SourceLine>
   <SourceLine>RightsMap.Value("Right.*") = True</SourceLine>
   <SourceLine>Elseif priv = "SELECT" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightSelect")=True</SourceLine>
   <SourceLine>Elseif priv = "INSERT" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightInsert")=True</SourceLine>
   <SourceLine>Elseif priv = "UPDATE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightUpdate")=True</SourceLine>
   <SourceLine>Elseif priv = "DELETE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightDelete")=True</SourceLine>
   <SourceLine>Elseif priv = "EXECUTE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightExecute")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE TEMPORARY TABLES" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateTempTable")=True </SourceLine>
   <SourceLine>Elseif priv = "CREATE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreate")=True</SourceLine>
   <SourceLine>Elseif priv = "DROP" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightDrop")=True</SourceLine>
   <SourceLine>Elseif priv = "ALTER" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightAlter")=True</SourceLine>
   <SourceLine>Elseif priv = "INDEX" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightIndex")=True</SourceLine>
   <SourceLine>Elseif priv = "TRIGGER" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightTrigger")=True</SourceLine>
   <SourceLine>Elseif priv = "REFERENCES" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightReferences")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE TABLESPACE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateTablespace")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE PIPELINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreatePipeline")=True</SourceLine>
   <SourceLine>Elseif priv = "DROP PIPELINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightDropPipeline")=True</SourceLine>
   <SourceLine>Elseif priv = "ALTER PIPELINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightAlterPipeline")=True</SourceLine>
   <SourceLine>Elseif priv = "START PIPELINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightStartPipeline")=True</SourceLine>
   <SourceLine>Elseif priv = "SHOW PIPELINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightShowPipeline")=True</SourceLine>
   <SourceLine>Elseif priv = "RELOAD" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightReload")=True</SourceLine>
   <SourceLine>Elseif priv = "SHUTDOWN" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightShutdown")=True</SourceLine>
   <SourceLine>Elseif priv = "FILE READ" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightFileRead")=True</SourceLine>
   <SourceLine>Elseif priv = "FILE WRITE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightFileWrite")=True</SourceLine>
   <SourceLine>Elseif priv = "PROCESS" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightProcess")=True</SourceLine>
   <SourceLine>Elseif priv = "SUPER" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightSuper")=True</SourceLine>
   <SourceLine>Elseif priv = "LOCK TABLES" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightLockTables")=True</SourceLine>
   <SourceLine>Elseif priv = "SHOW DATABASES" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightShowDatabase")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE USER" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateUser")=True</SourceLine>
   <SourceLine>Elseif priv = "EVENT" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightEvent")=True</SourceLine>
   <SourceLine>Elseif priv = "REPLICATION CLIENT" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightReplClient")=True</SourceLine>
   <SourceLine>Elseif priv = "REPLICATION SLAVE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightReplSlave")=True </SourceLine>
   <SourceLine>Elseif priv = "CREATE VIEW" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateView")=True</SourceLine>
   <SourceLine>Elseif priv = "SHOW VIEW" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightShowView")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE ROUTINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateRoutine")=True</SourceLine>
   <SourceLine>Elseif priv = "ALTER ROUTINE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightAlterRoutine")=True</SourceLine>
   <SourceLine>Elseif priv = "ALTER VIEW" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightAlterView")=True</SourceLine>
   <SourceLine>Elseif priv = "DROP VIEW" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightDropView")=True</SourceLine>
   <SourceLine>Elseif priv = "SHOW METADATA" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightShowMetadata")=True</SourceLine>
   <SourceLine>Elseif priv = "CLUSTER" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCluster")=True</SourceLine>
   <SourceLine>Elseif priv = "BACKUP" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightBackup")=True</SourceLine>
   <SourceLine>Elseif priv = "CREATE DATABASE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightCreateDatabase")=True</SourceLine>
   <SourceLine>Elseif priv = "DROP DATABASE" Then </SourceLine>
   <SourceLine>RightsMap.Value("RightDropDatabase")=True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If  Grant Then</SourceLine>
   <SourceLine>RightsMap.Value("RightGrant")=True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If UBound(rightsList) &gt; -1 Then</SourceLine>
   <SourceLine>RightsMap.Value("Usage")=True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
