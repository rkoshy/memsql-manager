 <Method>
  <ItemName>LoadDBList</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1529632767</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub LoadDBList()</SourceLine>
   <SourceLine>Dim  results As RecordSet</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>results = app.db.SQLSelect("SHOW DATABASES;")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If app.db.Error Then</SourceLine>
   <SourceLine>MsgBox("Error while doing SHOW DATABASES: "+app.db.ErrorMessage)</SourceLine>
   <SourceLine>Return </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If results &lt;&gt; Nil And results.RecordCount&gt;0 Then</SourceLine>
   <SourceLine>While Not results.eof</SourceLine>
   <SourceLine>If Not KnownDatabases.HasKey(results.IdxField(1).StringValue) Then</SourceLine>
   <SourceLine>KnownDatabases.Value(results.IdxField(1).StringValue) =True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>results.MoveNext</SourceLine>
   <SourceLine>Wend</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>results.Close</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In KnownDatabases</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim found As Boolean = False</SourceLine>
   <SourceLine>For i = 1 To PermissionScopes.PanelCount-1 </SourceLine>
   <SourceLine>If PermissionScopes.Caption(i) = entry.Key Then</SourceLine>
   <SourceLine>found = True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>If Not found Then</SourceLine>
   <SourceLine>PermissionScopes.Append(entry.key)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim tabNum As Integer</SourceLine>
   <SourceLine>tabNum = PermissionScopes.PanelCount - 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim sp As New SchemaPermissions</SourceLine>
   <SourceLine>sp.EmbedWithinPanel(PermissionScopes, tabNum, 20, 38, 698,488)</SourceLine>
   <SourceLine>SchemaPerms.Value(entry.key)=sp</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
