   <SourceLine>If (RightShowView.Value Or RightsGroupProcView.Value) And (Not ExistingRights.HasKey("RightShowView") And (Not ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>grant.Append("SHOW VIEW")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If (RightAlterRoutine.Value Or RightsGroupProcView.Value) And (Not ExistingRights.HasKey("RightAlterRoutine") And (Not ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>grant.Append("ALTER ROUTINE")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Not RightCreateView.Value And Not RightsGroupProcView.Value And (ExistingRights.HasKey("RightCreateView") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("CREATE VIEW")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightCreateRoutine.Value And Not RightsGroupProcView.Value And (ExistingRights.HasKey("RightCreateRoutine") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("CREATE ROUTINE")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightShowView.Value And Not RightsGroupProcView.Value And (ExistingRights.HasKey("RightShowView") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("SHOW VIEW")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightAlterRoutine.Value And Not RightsGroupProcView.Value And (ExistingRights.HasKey("RightAlterRoutine") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("ALTER ROUTINE")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
