   <SourceLine>grant.Append("CREATE")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If (RightDrop.Value Or RightsGroupDML.Value) And (Not ExistingRights.HasKey("RightDrop") And (Not ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
