 <ControlBehavior>
  <Superclass>PushButton</Superclass>
  <HookInstance>
   <ItemName>Action</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>1898694655</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>app.db.SQLExecute("BEGIN TRANSACTION")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If username.Text.Trim&lt;&gt;"" And Hostname.Text.Trim&lt;&gt;"" Then</SourceLine>
    <SourceLine>Dim existing As Boolean = False</SourceLine>
    <SourceLine>Dim newpassword As Boolean = False</SourceLine>
    <SourceLine>If UserName.Text = userlist.Cell(userlist.ListIndex,0) And HostName.Text= userlist.Cell(userlist.ListIndex,1)  Then</SourceLine>
    <SourceLine>// This is an existing user (that's selected on the left side)</SourceLine>
    <SourceLine>existing = True</SourceLine>
    <SourceLine>newpassword = (Password.Text &lt;&gt; "!!&lt;&lt;NOCHANGE&gt;&gt;!!")</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>If Password.Text.Trim="" Then</SourceLine>
    <SourceLine>MsgBox("A password is required when saving a new user")</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>newpassword =True</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>Dim SQL(-1) As String</SourceLine>
    <SourceLine>GlobalPerms.GetSQLStatements(Username.Text,Hostname.Text, Password.Text, newpassword, SQL)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Update the GUI</SourceLine>
    <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In UserRights</SourceLine>
    <SourceLine>If CType(entry.key,String) &lt;&gt; "Global" Then</SourceLine>
    <SourceLine>Dim sp As SchemaPermissions = SchemaPerms.Value(entry.key)</SourceLine>
    <SourceLine>sp.GetSQLStatements(Username.Text,Hostname.Text,CType(entry.key, String), SQL)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>System.DebugLog("Executing SQL:")</SourceLine>
    <SourceLine>For Each stmt As String In SQL</SourceLine>
    <SourceLine>System.DebugLog(stmt)</SourceLine>
    <SourceLine>app.db.SQLExecute(stmt)</SourceLine>
    <SourceLine>If app.db.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while executing: "+stmt+" ===&gt; "+app.db.ErrorMessage)</SourceLine>
    <SourceLine>app.db.Rollback</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.db.Commit</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while committing transaction"+app.db.ErrorMessage)</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// No errors, if it's a new user - add them to the list</SourceLine>
    <SourceLine>If Not existing Then</SourceLine>
    <SourceLine>UserList.AddRow(Username.Text,Hostname.Text)</SourceLine>
    <SourceLine>UserList.ListIndex=UserLIst.ListCount-1</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>GetUserDetails</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
