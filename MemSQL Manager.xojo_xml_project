   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>555481087</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>If connectionList.ListIndex &gt;= 0 Then</SourceLine>
    <SourceLine>if MsgBox("Are you sure you want to delete the saved connection: "+connectionList.Cell(connectionList.ListIndex,0), 4 +32+256,"Delete Connection")=6 then</SourceLine>
    <SourceLine>If Not app.settings.Connect Then</SourceLine>
    <SourceLine>MsgBox("Error opening settings file!")</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.SQLExecute("DELETE FROM connection WHERE name = '"+connectionList.Cell(connectionList.ListIndex,0)+"'")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.settings.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to delete connection: " + app.settings.ErrorMessage)</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.settings.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to delete connection - commit failed: " + app.settings.ErrorMessage)</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.Close</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>LoadConnectionList</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
