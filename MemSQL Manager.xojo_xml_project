 <Method>
  <ItemName>GetSQLStatements</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1289603071</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub GetSQLStatements(Username as String, Hostname as String, Password as String, NewPassword as Boolean, SQL() as String)</SourceLine>
   <SourceLine>consolidateRights</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim OptionalPassword As String = ""</SourceLine>
   <SourceLine>If NewPassword Then</SourceLine>
   <SourceLine>OptionalPassword = " IDENTIFIED BY '"+Password+"'"</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim Grant(-1) As String</SourceLine>
   <SourceLine>Dim Revoke(-1) As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ProcessPrivileges(grant,revoke)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Ubound(Grant) &gt; -1 Then</SourceLine>
   <SourceLine>SQL.append("GRANT "+Join(Grant,", ")+" ON *.* TO '"+username+"'@'"+hostname+"'"+OptionalPassword+GetGrantOption+";")</SourceLine>
   <SourceLine>ElseIf NewPassword Then</SourceLine>
   <SourceLine>SQL.append("GRANT USAGE ON *.* TO '"+username+"'@'"+hostname+"' "+OptionalPassword+GetGrantOption+";")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Ubound(Revoke) &gt; -1 Then</SourceLine>
   <SourceLine>SQL.append("REVOKE "+Join(Revoke,", ")+" ON *.* FROM '"+username+"'@'"+hostname+"';")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If (ExistingRights.HasKey("RightGrant") Or ExistingRights.HasKey("Right.*") Or ExistingRights.HasKey("RightsGroup.*")) And GetGrantOption = "" Then</SourceLine>
   <SourceLine>SQL.append("REVOKE GRANT OPTION ON *.* FROM '"+username+"'@'"+hostname+"';")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>Username as String, Hostname as String, Password as String, NewPassword as Boolean, SQL() as String</ItemParams>
  <ItemResult></ItemResult>
