  <ItemName>ProcessPrivileges</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>922920959</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub ProcessPrivileges(Grant() as String, Revoke() as String)</SourceLine>
   <SourceLine>If RightsGroupAdmin.Value And RightsGroupBasic.Value And RightsGroupDML.Value And RightsGroupPipeline.Value And RightsGroupProcView.Value Then</SourceLine>
   <SourceLine>grant.append("ALL PRIVILEGES")</SourceLine>
   <SourceLine>return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ProcessBasicPrivileges(grant,revoke)</SourceLine>
   <SourceLine>ProcessDMLPrivileges(grant,revoke)</SourceLine>
   <SourceLine>ProcessPipelinePrivileges(grant,revoke)</SourceLine>
   <SourceLine>ProcessProcViewPrivileges(grant,revoke)</SourceLine>
   <SourceLine>ProcessAdminPrivileges(grant,revoke)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
