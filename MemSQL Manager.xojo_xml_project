   <SourceLine>grant.Append("INDEX")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If (RightTrigger.Value Or RightsGroupDML.Value) And (Not ExistingRights.HasKey("RightTrigger") And (Not ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>grant.Append("TRIGGER")</SourceLine>
   <SourceLine>End If</SourceLine>
