    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>if app.isConnected then</SourceLine>
    <SourceLine>app.db.Close</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.db = new MySQLCommunityServer</SourceLine>
    <SourceLine>app.db.Host = Hostname.Text</SourceLine>
    <SourceLine>app.db.Port = Port.Text.Val</SourceLine>
    <SourceLine>app.db.UserName = Username.Text</SourceLine>
    <SourceLine>app.db.Password = Password.Text</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Connect Then</SourceLine>
    <SourceLine>app.isConnected = true</SourceLine>
    <SourceLine>MsgBox("Connection was successful")</SourceLine>
    <SourceLine>app.db.Close</SourceLine>
    <SourceLine>Save.Enabled=True</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>app.isConnected = false</SourceLine>
    <SourceLine>MsgBox(app.db.ErrorMessage)</SourceLine>
    <SourceLine>Save.Enabled=False</SourceLine>
