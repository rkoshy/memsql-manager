   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Change()</SourceLine>
    <SourceLine>If connectionList.ListIndex &gt;= 0 Then</SourceLine>
    <SourceLine>If Not app.settings.Connect Then</SourceLine>
    <SourceLine>MsgBox("Error opening settings file!")</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim rs As RecordSet</SourceLine>
    <SourceLine>rs = app.settings.SQLSelect("SELECT name,hostname,port,username,password FROM connection WHERE name = '"+connectionList.Cell(connectionList.ListIndex,0)+"'")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.settings.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to get connection list: " + app.settings.ErrorMessage)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If rs &lt;&gt; Nil Then</SourceLine>
    <SourceLine>ConnectionName.Text=rs.Field("name")</SourceLine>
    <SourceLine>Hostname.Text=rs.Field("hostname")</SourceLine>
    <SourceLine>Port.Text=rs.Field("port")</SourceLine>
    <SourceLine>Username.Text=rs.Field("username")</SourceLine>
    <SourceLine>Password.Text=rs.Field("password")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>rs.Close</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.Close</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
  <HookInstance>
   <ItemName>DoubleClick</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>879816703</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub DoubleClick()</SourceLine>
    <SourceLine>If connectionList.ListIndex &gt;= 0 Then</SourceLine>
    <SourceLine>If Not app.settings.Connect Then</SourceLine>
    <SourceLine>MsgBox("Error opening settings file!")</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim rs As RecordSet</SourceLine>
    <SourceLine>rs = app.settings.SQLSelect("SELECT name,hostname,port,username,password FROM connection WHERE name = '"+connectionList.Cell(connectionList.ListIndex,0)+"'")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.settings.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to get connection list: " + app.settings.ErrorMessage)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If rs &lt;&gt; Nil Then</SourceLine>
    <SourceLine>ConnectionName.Text=rs.Field("name")</SourceLine>
    <SourceLine>Hostname.Text=rs.Field("hostname")</SourceLine>
    <SourceLine>Port.Text=rs.Field("port")</SourceLine>
    <SourceLine>Username.Text=rs.Field("username")</SourceLine>
    <SourceLine>Password.Text=rs.Field("password")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>rs.Close</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.Close</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.db = New MySQLCommunityServer</SourceLine>
    <SourceLine>app.db.Host = Hostname.Text</SourceLine>
    <SourceLine>app.db.Port = Port.Text.Val</SourceLine>
    <SourceLine>app.db.DatabaseName = "platform28"</SourceLine>
    <SourceLine>app.db.UserName = Username.Text</SourceLine>
    <SourceLine>app.db.Password = Password.Text</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Connect Then</SourceLine>
    <SourceLine>app.isConnected = True</SourceLine>
    <SourceLine>ConnectionWindow.Hide</SourceLine>
    <SourceLine>MainWindow.Show</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>app.isConnected = False</SourceLine>
    <SourceLine>MsgBox(app.db.ErrorMessage)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
