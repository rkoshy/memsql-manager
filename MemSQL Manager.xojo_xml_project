 <Method>
  <ItemName>LoadUserList</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1119893503</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub LoadUserList()</SourceLine>
   <SourceLine>Dim  users As RecordSet</SourceLine>
   <SourceLine>users = app.db.SQLSelect("SHOW USERS;")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if app.db.Error then</SourceLine>
   <SourceLine>MsgBox("Error while searching: "+app.db.ErrorMessage)</SourceLine>
   <SourceLine>return </SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if users= nil or users.RecordCount=0 then</SourceLine>
   <SourceLine>MsgBox("No results")</SourceLine>
   <SourceLine>return </SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>UserList.DeleteAllRows</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>while not users.eof</SourceLine>
   <SourceLine>dim Temp as String = users.Field("User").StringValue.ReplaceAll("'","")</SourceLine>
   <SourceLine>dim Temp2(-1) as String = Temp.Split("@")</SourceLine>
   <SourceLine>UserList.addRow(Temp2(0), Temp2(1))</SourceLine>
   <SourceLine>users.MoveNext</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>users.Close</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
