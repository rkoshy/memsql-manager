    <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In KnownDatabases</SourceLine>
    <SourceLine>UserRights.Value(entry.key) = New Xojo.Core.Dictionary</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>GlobalPerms.clear</SourceLine>
    <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In KnownDatabases</SourceLine>
    <SourceLine>Dim sp As SchemaPermissions =SchemaPerms.Value(entry.key)</SourceLine>
    <SourceLine>sp.clear</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Username.Text=""</SourceLine>
    <SourceLine>Password.Text=""</SourceLine>
    <SourceLine>Hostname.Text=""</SourceLine>
    <SourceLine>UserList.ListIndex=-1</SourceLine>
    <SourceLine>End Sub</SourceLine>
