  <ItemName>setPermissions</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1077477375</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub setPermissions(perms as Xojo.Core.Dictionary)</SourceLine>
   <SourceLine>For Each entry As Xojo.Core.DictionaryEntry In perms</SourceLine>
   <SourceLine>Dim rg As New RegEx</SourceLine>
   <SourceLine>Dim myMatch As RegExMatch</SourceLine>
   <SourceLine>rg.SearchPattern = "^"+entry.key+"$"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim i, c As Integer</SourceLine>
   <SourceLine>Dim obj As Control</SourceLine>
   <SourceLine>c = Self.ControlCount - 1</SourceLine>
   <SourceLine>For i = 0 To c Step 1</SourceLine>
   <SourceLine>obj = Self.Control(i)</SourceLine>
   <SourceLine>myMatch = rg.Search(obj.Name)</SourceLine>
   <SourceLine>If myMatch&lt;&gt;Nil And obj IsA CheckBox Then</SourceLine>
   <SourceLine>Dim objButton As CheckBox = CheckBox(obj)</SourceLine>
   <SourceLine>objButton.Value=true</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>consolidateRights</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
