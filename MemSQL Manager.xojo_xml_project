    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>app.db = New MySQLCommunityServer</SourceLine>
    <SourceLine>app.db.Host = Hostname.Text</SourceLine>
    <SourceLine>app.db.Port = Port.Text.Val</SourceLine>
    <SourceLine>app.db.DatabaseName = "platform28"</SourceLine>
    <SourceLine>app.db.UserName = Username.Text</SourceLine>
    <SourceLine>app.db.Password = Password.Text</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Connect Then</SourceLine>
    <SourceLine>app.isConnected = true</SourceLine>
    <SourceLine>ConnectionWindow.Hide</SourceLine>
    <SourceLine>MainWindow.Show</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>app.isConnected = false</SourceLine>
    <SourceLine>MsgBox(app.db.ErrorMessage)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
 <ControlBehavior>
  <Superclass>PushButton</Superclass>
  <HookInstance>
   <ItemName>Action</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>2066769919</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>If ConnectionName.Text.Trim()="" then</SourceLine>
    <SourceLine>MsgBox("Cannot have an empty connection name when saving")</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>ElseIf Hostname.Text.Trim()="" then</SourceLine>
    <SourceLine>MsgBox("Cannot have an empty hostname when saving")</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>elseif Username.Text.Trim()="" then</SourceLine>
    <SourceLine>MsgBox("Cannot have an empty username when saving")</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>End if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if Port.Text.Trim="" then</SourceLine>
    <SourceLine>Port.Text="3306"</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If Not app.settings.Connect Then</SourceLine>
    <SourceLine>MsgBox("Error opening settings file!")</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.SQLExecute("INSERT INTO connection(name,hostname,port,username,password) values ('" _</SourceLine>
    <SourceLine>+ConnectionName.Text.Trim()+"','" _</SourceLine>
    <SourceLine>+Hostname.Text.Trim()+"','" _</SourceLine>
    <SourceLine>+Port.Text.Trim()+"','" _</SourceLine>
    <SourceLine>+Username.Text.Trim()+"','" _</SourceLine>
    <SourceLine>+Password.Text.Trim()+"');")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.settings.Error Then</SourceLine>
    <SourceLine>if app.settings.ErrorMessage &lt;&gt;"UNIQUE constraint failed: connection.name" then</SourceLine>
    <SourceLine>MsgBox("Error while trying to save connection: " + app.settings.ErrorMessage)</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>else </SourceLine>
    <SourceLine>MsgBox("Cannot save - the connection name already exists!")</SourceLine>
    <SourceLine>return</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>connectionList.AddRow(ConnectionName.Text.Trim)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.settings.Close</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Save.Enabled=false</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
