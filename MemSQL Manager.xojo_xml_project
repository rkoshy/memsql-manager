   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightDrop.Value And Not RightsGroupDML.Value And (ExistingRights.HasKey("RightDrop") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("DROP")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightAlter.Value And Not RightsGroupDML.Value And (ExistingRights.HasKey("RightAlter") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("ALTER")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightIndex.Value And Not RightsGroupDML.Value And (ExistingRights.HasKey("RightIndex") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("INDEX")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Not RightTrigger.Value And Not RightsGroupDML.Value And (ExistingRights.HasKey("RightTrigger") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("TRIGGER")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If (Not RightReferences.Value And Not RightsGroupDML.Value) And (ExistingRights.HasKey("RightReferences") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("REFERENCES")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If (Not RightCreateTablespace.Value And Not RightsGroupDML.Value) And (ExistingRights.HasKey("RightCreateTablespace") Or (ExistingRights.HasKey("RightsGroup.*"))) Then</SourceLine>
   <SourceLine>revoke.Append("CREATE TABLESPACE")</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
