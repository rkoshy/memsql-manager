    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>If userList.ListIndex &gt;= 0 Then</SourceLine>
    <SourceLine>If MsgBox("Are you sure you want to delete the user: "+userList.Cell(userList.ListIndex,0) + "@"+userList.Cell(userList.ListIndex,1), 4 +32+256,"Delete User")=6 Then</SourceLine>
    <SourceLine>app.db.SQLExecute("DROP USER '"+userList.Cell(userList.ListIndex,0) + "'@'"+userList.Cell(userList.ListIndex,1)+"'")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to delete user: " + app.db.ErrorMessage)</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>app.db.Commit</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If app.db.Error Then</SourceLine>
    <SourceLine>MsgBox("Error while trying to commit user deletion: " + app.db.ErrorMessage)</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>LoadUserList</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
